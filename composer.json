{
    "name": "flub/bigbang-bundle",
    "description": "Utility classes for speed up your Symfony dev",
    "type": "symfony-bundle",
    "keywords": ["rad", "utility"],
    "license": "MIT",
    "authors": [
        {
            "name": "Florian Lonqueu-Brochard"
        }
    ],
    "require": {
        "php": ">=5.3.3",
        "symfony/symfony": "~2.3"
    },
    "require-dev": {
        "nelmio/alice": "dev-master",
        "composer/composer": "dev-master",
        "liip/rmt": "1.0.*"
    },
    "suggest": {
        "nelmio/alice": "To create expressive fixtures/fake data",
        "composer/composer": "To use automatic update of the init command"
    },
    "autoload": {
        "psr-0": {
            "Flub\\BigBangBundle": ""
        }
    },
    "target-dir": "Flub/BigBangBundle",
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    }
}
